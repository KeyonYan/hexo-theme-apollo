doctype
html(lang=config.language)
	head
		include head
	body
		.wrap
			header
				a.logo-link(href=url_for())
					img(src=url_for(theme.logo) alt="logo")
				.iconName(class={active: act})!= config.author
				include nav
			main.container
				block container
			footer
				block pagination
				block copyright
		include scripts
